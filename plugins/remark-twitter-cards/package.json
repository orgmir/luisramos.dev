{
  "name": "remark-twitter-cards",
  "version": "0.5.0",
  "description": "Gatsby Remark plugin for generating Twitter Open Graph cards",
  "repository": "https://github.com/alessbell/gatsby-remark-twitter-cards",
  "author": "Alessia Bellisario",
  "license": "MIT",
  "private": false,
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "remark",
    "social",
    "sharing",
    "twitter",
    "card",
    "graphic",
    "image"
  ],
  "scripts": {
    "prebuild": "npm run lint",
    "build": "cross-env BABEL_ENV=production babel src --out-dir .",
    "lint": "eslint --ext .js src",
    "prepublishOnly": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "jimp": "^0.14.0",
    "path": "^0.12.7",
    "wasm-twitter-card": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "10.1.0",
    "babel-preset-env": "1.7.0",
    "cross-env": "7.0.2",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "husky": "4.3.8",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5"
  },
  "peerDependencies": {
    "gatsby": ">=2.0.0"
  }
}
